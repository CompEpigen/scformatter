rule S4toH5AD:
    input:
        script = "scripts/S4toH5AD.R",
        object = "rawdata/seurat4"
    output:
        h5ad = directory("procdata/h5ad")
    container:
        "docker://satijalab/seurat:4.0.1"
    shell:
        """
        mkdir -p logs
	Rscript {input.script} {input.object} {output.h5ad} > logs/S4toH5AD.Rout 2>&1
        """

